.container{
  background: #fff;
  margin-top: 20rpx;
}

.icon-image{
  width: 38rpx;
  height:38rpx;
  margin-right: 15rpx;
  margin-left: 15rpx;
}

.icon-view{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-left:15rpx
}

.report-name{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding-top: 15rpx;
  padding-bottom: 15rpx; 
}

.report-view-line{
 border-bottom: 1rpx solid #f1f1f1;
}

.report-version{
  border-radius: 8rpx;
  background: #e3b96f;
  color: #fff;
  font-size: 10px;
  padding: 5rpx;
  margin-left: 15rpx;
}

.icon-image-min{
  width: 25rpx;
  height:25rpx;
  margin-right: 15rpx;
  margin-left: 15rpx;
}

.delete-view{
  border: 1rpx solid #2b2b2b;
  border-radius: 18rpx;
  padding: 8rpx;
  margin: 15rpx;
}

.operation{
  display: flex;
  flex-direction: row-reverse;
  align-items: center;

}

.view-hide{
    display: none;
}

.view-show{
    display: flex;
}

.collection{
   background: url('https://m.zmbeidiao.com/img/wxapplet/icon_collection.png') no-repeat;
   background-size: 25rpx 25rpx;
}

.urge-dis{
  background: url('https://m.zmbeidiao.com/img/wxapplet/icon_urge_dis.png') no-repeat;
  background-size: 25rpx 25rpx;
}

.message-empty{
  width: 230rpx;
  height: 230rpx;
}

.empty-view-show{
  width: 100%;
  height: 98vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
 }

 .empty-view-hidden{
  width: 100%;
  height: 0vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
 }
