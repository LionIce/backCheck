.item-report{
  height: 160rpx;  
  width: 660rpx;  
  border: 1rpx soild #fff;
  background-color: #fff; 
  border-radius: 12rpx;
  display: flex;
  margin-top: 300rpx;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.bg-config{
  width: 100%;
  height: 460rpx;  
  background: url('https://m.zmbeidiao.com/img/wxapplet/bg_config.png') no-repeat;
  background-size: 100% 380rpx; 
}

.view-content{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.item-menu{
  width: 219rpx;
}

.item-menu-icon{
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 20rpx;
}

.model-title-icon{ 
  width: 10rpx;
  height: 30rpx;
  border: 1rpx soild #e3b96f;
  background-color: #e3b96f; 
  border-radius: 8rpx;
  margin-left: 45rpx
}

.model-title{
  display: flex;  
  flex-direction: row; 
  align-items: center;
  margin-top: 25rpx;
}

.model-item{

  display: flex;  
  flex-direction: column; 
}

.model-item-image{
  height: 164rpx;  
  width: 660rpx;  
  border-radius: 12rpx;
  margin-top: 25rpx;  
  margin-left: 45rpx;
  padding-right: 45rpx;
  background: url('https://m.zmbeidiao.com/img/wxapplet/pic_ji_chu_ban.png') no-repeat;
  background-size:  660rpx 164rpx; 
}

.model-max-width{ 
  width: 100%;
}

.model-item-text{
  margin-top: 30rpx;
}

.model-item-content{
  display: flex;  
  flex-direction: row;
  height: 15px;
  width: 610rpx;  
  margin-top: 25rpx;
}

text{
  overflow:hidden; 
  text-overflow:ellipsis;
  white-space:nowrap; 
}