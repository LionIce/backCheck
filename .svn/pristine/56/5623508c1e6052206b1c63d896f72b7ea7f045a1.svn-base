.basis-view{
  display: flex;
  flex-direction: row;
  align-items: center;
 
}

.icon-image{
    width: 20rpx;
    height: 32rpx;
    padding-right: 20rpx;
}

input{
  width: 90%;
  font-size: 14px;
  height: 70rpx
}


.input-view{
  background: #fff;
  border-radius: 6px;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 5px;
  padding-bottom: 5px;
  padding-left:15px; 
  margin-top : 25rpx; 
}

button{
    border-radius : 12rpx;
    background-color: #e3b96f;
    color: #fff;
    width: 100%;
    margin-top: 20px;
}

.button-view{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center
}

.swiper {
  height: 400rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}

.report-item{
  width: 100%;
  height: 350rpx;
  border-radius: 12rpx;
  background: url("https://m.zmbeidiao.com/img/wxapplet/bg_option.png") no-repeat;
  background-size:  100% 350rpx;
  background-position-x:-5px; 
  background-position-x:5px; 
  padding-top: 5rpx;
}

.login-button{
    border-radius : 12rpx;
    background-color: #e3b96f;
    color: #fff;
}

.margin-left {
		display: -moz-inline-box;
		display: inline-block;
		white-space : normal nowrap; 
    margin-top: 15rpx;
    margin-left: 15rpx;
    margin-right: 15rpx;
    font-size: 12px;
    color: #fff
}

.tplName-view{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:space-between;
  border-bottom: 1rpx solid #e3b96f;
  margin: 20rpx;
}

.tplName-text{
  color: #e3b96f;
  font-size: 14px;
}

.chargeAmt-text{
  margin: 10rpx;
  color: #e3b96f;
  font-size: 12px;
}
